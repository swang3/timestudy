outlier<-function(data,x,x_colname){
library(tidyverse)
library(knitr)
lim1<-data[,x]%>%
quantile()%>%
unname()[4]
lim2<-data[,x]%>%
quantile()%>%
unname()[4]
lim3<-IQR(data[,x])*1.5
lim1<-lim1+lim3
lim2<-lim2-lim3
outliers<-filter(x_colname>lim1|x_colname<lim2)
if(nrow(outlier)==0)
print("there is no outliers")
else
kable(outliers,caption = outliers)
}
